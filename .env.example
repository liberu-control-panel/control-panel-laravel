APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=myapp
DB_USERNAME=myapp
DB_PASSWORD=

# PostgreSQL configuration (alternative to MySQL)
# To use PostgreSQL, set DB_CONNECTION=pgsql and DB_HOST=postgresql
# DB_CONNECTION=pgsql
# DB_HOST=postgresql
# DB_PORT=5432
# DB_DATABASE=myapp
# DB_USERNAME=myapp
# DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_DRIVER=cookie
SESSION_DOMAIN=null
SESSION_SECURE_COOKIE=false

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_ENDPOINT=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Docker environment variables
DOCKER_IMAGE=control-panel-laravel
CONTROL_PANEL_DOMAIN=control-panel.example.com
LETSENCRYPT_EMAIL=admin@example.com

# Kubernetes Configuration
KUBERNETES_ENABLED=true
KUBECTL_PATH=/usr/local/bin/kubectl
KUBERNETES_NAMESPACE_PREFIX=hosting-
KUBERNETES_INGRESS_CLASS=nginx
KUBERNETES_CERT_ISSUER=letsencrypt-prod
KUBERNETES_STORAGE_CLASS=standard

# SSH Connection Configuration
SSH_TIMEOUT=30
SSH_RETRY_ATTEMPTS=3
SSH_RETRY_DELAY=5
SSH_CONNECTION_POOL_SIZE=10
SSH_KEEPALIVE_INTERVAL=60
SSH_SUDO_ENABLED=false
SSH_SUDO_PASSWORD_REQUIRED=true

# OAuth Configuration for Private Repository Access
# GitHub OAuth - Get credentials from https://github.com/settings/developers
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI="${APP_URL}/oauth/github/callback"

# GitLab OAuth - Get credentials from https://gitlab.com/-/profile/applications
GITLAB_CLIENT_ID=
GITLAB_CLIENT_SECRET=
GITLAB_REDIRECT_URI="${APP_URL}/oauth/gitlab/callback"
GITLAB_INSTANCE_URI=https://gitlab.com

# Managed Database Configuration
# AWS RDS/Aurora
AWS_RDS_ENABLED=false
AWS_RDS_DEFAULT_INSTANCE_CLASS=db.t3.micro
AWS_RDS_DEFAULT_STORAGE=20
AWS_RDS_STORAGE_ENCRYPTED=true
AWS_RDS_BACKUP_RETENTION=7
AWS_RDS_MULTI_AZ=false
AWS_RDS_PUBLIC=false

# Azure Database
AZURE_DATABASE_ENABLED=false
AZURE_SUBSCRIPTION_ID=
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_RESOURCE_GROUP=
AZURE_DEFAULT_REGION=eastus
AZURE_DB_DEFAULT_SKU=B_Gen5_1
AZURE_DB_DEFAULT_TIER=Basic
AZURE_DB_DEFAULT_STORAGE=5120
AZURE_DB_BACKUP_RETENTION=7
AZURE_DB_SSL_ENFORCEMENT=Enabled

# DigitalOcean Managed Database
DO_DATABASE_ENABLED=false
DO_API_TOKEN=
DO_DEFAULT_REGION=nyc3
DO_DB_DEFAULT_SIZE=db-s-1vcpu-1gb
DO_DB_DEFAULT_NODES=1

# OVH Managed Database
OVH_DATABASE_ENABLED=false
OVH_APPLICATION_KEY=
OVH_APPLICATION_SECRET=
OVH_CONSUMER_KEY=
OVH_ENDPOINT=ovh-eu
OVH_SERVICE_NAME=
OVH_DEFAULT_REGION=GRA
OVH_DB_DEFAULT_PLAN=essential

# Google Cloud SQL
GCP_SQL_ENABLED=false
GCP_PROJECT_ID=
GCP_CREDENTIALS_PATH=
GCP_DEFAULT_REGION=us-central1
GCP_SQL_DEFAULT_TIER=db-f1-micro
GCP_SQL_AUTO_RESIZE=true
GCP_SQL_MAX_STORAGE=0
GCP_SQL_BACKUP_ENABLED=true
GCP_SQL_REQUIRE_SSL=true

# Managed Database Global Settings
MANAGED_DB_AUTO_TEST=true
MANAGED_DB_TIMEOUT=300
MANAGED_DB_ENFORCE_SSL=true
MANAGED_DB_CACHE_TTL=3600
